{
  "name": "nuxt-auto-resting-with-circleci",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "Yusuke_Saito <ysaito8015@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "generate": "nuxt generate",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "nuxt": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@vue/test-utils": "1",
    "@vue/vue2-jest": "28",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^29.1.2",
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^4.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jsdom-global": "^3.0.2",
    "lodash.clonedeep": "^4.5.0"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.(vue)$": "@vue/vue2-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/app/$1",
      "^@@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/app/$1",
      "^~~/(.*)$": "<rootDir>/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "app/**/*.{js,vue}"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  }
}
